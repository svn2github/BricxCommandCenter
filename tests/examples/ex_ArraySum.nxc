#include "cstdlib.h"
#include "display.h"
#include "command.h"

task main()
{
  long data[40];
  for (int i = 0; i < 40; i++)
    data[i] = rand();
  TextOut(0, LCD_LINE1, "Sum of values = ");
  long x = ArraySum(data, NA, NA); // start at 0 and go to length(data);
  NumOut(0, LCD_LINE2, x);
  Wait(SEC_3);
}
